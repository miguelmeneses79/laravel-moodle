{
    "name": "miguelmeneses79/laravel-moodle",
    "description": "Laravel Moodle client",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Miguel Meneses",
            "email": "miguel.meneses@wearekfactor.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "beberlei/assert": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "miguelmeneses79\\laravel-moodle\\LaravelMoodleServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "miguelmeneses79\\laravel-moodle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "miguelmeneses79\\laravel-moodle\\Tests\\": "tests/"
        }
    }
}
